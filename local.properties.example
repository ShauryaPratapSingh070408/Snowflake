# Copy this file to 'local.properties' and add your actual values
# This file should be in .gitignore and never committed

## Android SDK Location
# On Mac:
# sdk.dir=/Users/YOUR_USERNAME/Library/Android/sdk

# On Windows:
# sdk.dir=C:\\Users\\YOUR_USERNAME\\AppData\\Local\\Android\\Sdk

# On Linux:
# sdk.dir=/home/YOUR_USERNAME/Android/Sdk

sdk.dir=/path/to/Android/Sdk

## Gemini API Key (REQUIRED for Phase 1)
# Get your free API key from: https://makersuite.google.com/app/apikey
# This is needed for the AI chat feature to work
GEMINI_API_KEY=your_actual_gemini_api_key_here

# Example (DO NOT use this, get your own):
# GEMINI_API_KEY=AIzaSyA1B2C3D4E5F6G7H8I9J0K1L2M3N4O5P6Q